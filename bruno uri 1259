linhas = int(input("digite o numero de linhas: "))
par = []
impar = []
ordem = []
for i in  range(linhas):
    x = int(input("digite o valor"))
    if x % 2 == 0:
        par.append(x)
    else:
        impar.append(x)
par.sort()
impar.sort(reverse=True)
for i in par:
    ordem.append(i)
for i in impar:
    ordem.append(i)
for i in ordem:
    print(i)
